{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {        
        "src": "/(.*)",        
        "dest": "server.js"
      }
    ],
    "build": {
      "command": "npm run build",
      "output": "build"
    }
  
  }